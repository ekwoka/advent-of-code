{
  "exclude": [
    "**/node_modules"
  ],
  "compilerOptions": {
    "types": [
      "vitest/globals",
      "bun-types",
      "@ekwoka/rust-ts/prelude"
    ],
    "noEmit": true,
    "declaration": false,
    "declarationMap": false,
    "esModuleInterop": true,
    "module": "es2022",
    "outDir": "dist",
    "skipLibCheck": true,
    "strict": true,
    "target": "esNext",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "bundler"
  }
}
