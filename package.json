{
  "scripts": {
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "22.6.1",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "bun-types": "1.1.29",
    "eslint": "8.56.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "prettier-plugin-rust": "0.1.9",
    "typescript": "5.6.2",
    "vite": "5.4.7",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.1"
  },
  "lint-staged": {
    "*.{js,ts,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,html,css,scss,less,graphql,yml,yaml}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSameLine": true,
    "plugins": [
      "prettier-plugin-rust"
    ]
  },
  "pnpm": {
    "overrides": {
      "semver@<5.7.2": ">=5.7.2",
      "typescript@<5.3.3": ">=5.3.3"
    }
  },
  "dependencies": {
    "@ekwoka/rust-ts": "0.4.1",
    "hyperimport": "0.2.0"
  }
}
