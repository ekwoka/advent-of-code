{
  "scripts": {
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "20.12.12",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "bun-types": "1.1.10",
    "eslint": "8.56.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.5",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "prettier-plugin-rust": "0.1.9",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  },
  "lint-staged": {
    "*.{js,ts,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,html,css,scss,less,graphql,yml,yaml}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSameLine": true,
    "plugins": [
      "prettier-plugin-rust"
    ]
  },
  "pnpm": {
    "overrides": {
      "semver@<5.7.2": ">=5.7.2",
      "typescript@<5.3.3": ">=5.3.3"
    }
  },
  "dependencies": {
    "@ekwoka/rust-ts": "0.3.1",
    "hyperimport": "0.2.0"
  }
}
