{
  "scripts": {
    "lint": "biome check"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "22.7.9",
    "bun-types": "1.1.33",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "prettier-plugin-rust": "0.1.9",
    "typescript": "5.6.3",
    "vitest": "2.1.3"
  },
  "lint-staged": {
    "*.{js,ts,mjs}": [
      "biome check --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "semver@<5.7.2": ">=5.7.2",
      "typescript": "$typescript"
    }
  },
  "dependencies": {
    "@ekwoka/rust-ts": "0.5.0"
  }
}
