{
  "scripts": {
    "lint": "biome check"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@types/node": "22.7.4",
    "bun-types": "1.1.29",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "npm-run-all2": "6.2.3",
    "prettier": "3.3.3",
    "prettier-plugin-rust": "0.1.9",
    "typescript": "5.6.2",
    "vite": "5.4.8",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.1"
  },
  "lint-staged": {
    "*.{js,ts,mjs}": [
      "biome check --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "semver@<5.7.2": ">=5.7.2",
      "typescript": "$typescript"
    }
  },
  "dependencies": {
    "@ekwoka/rust-ts": "0.4.1",
    "hyperimport": "0.2.0"
  }
