{
  "scripts": {
    "precommit": "lint-staged",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --write ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "bun-types": "1.0.14",
    "eslint": "8.54.0",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.0",
    "prettier-plugin-rust": "0.1.9",
    "typescript": "5.3.2",
    "vite": "5.0.4",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "lint-staged": {
    "*.{js,ts,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,html,css,scss,less,graphql,yml,yaml}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSameLine": true,
    "plugins": [
      "prettier-plugin-rust"
    ]
  },
  "pnpm": {
    "overrides": {
      "semver@<5.7.2": ">=5.7.2"
    }
  },
  "dependencies": {
    "@ekwoka/rust-ts": "^0.0.5",
    "hyperimport": "^0.2.0"
  }
}
